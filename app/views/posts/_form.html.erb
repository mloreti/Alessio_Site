<%= form_for @post, html: { multipart: true } do |f| %>

  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.select :category, options_for_select([["Category", ""], "Architecture", "Action",  "Landscape", "People"]), :class => '' %>
  </p>

  <p>
    <%= f.label :image %>
    <%= f.file_field :image %>
  </p>

  <%= f.submit "Upload image" %>
<% end %>
